{
  "parserOptions": { "ecmaVersion": 6 },
  "extends": ["react-app", "eslint:recommended", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "semi": "off",
    "no-duplicate-imports": "error",
    "no-unused-vars": "warn",
    "react/no-unescaped-entities": "off",
    "react/prop-types": "warn"
  }
}
